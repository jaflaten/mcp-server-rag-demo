# Environment variables for MCP RAG Server
# Copy this file to .env and fill in your values

# OpenAI API Key (optional - only needed if using OpenAI embeddings/LLM)
OPENAI_API_KEY=

# Ollama API URL (default works for Ollama running on host machine)
OLLAMA_URL=http://host.docker.internal:11434

# Vector store file path
VECTOR_STORE_PATH=/app/data/vector_store.json

# Documents directory path
DOCUMENTS_PATH=/app/documents
